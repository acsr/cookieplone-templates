{
  "title": "Documentation Add-on",
  "description": "A new documentation add-on for Plone",
  "author": "Plone Community",
  "email": "collective@plone.org",
  "github_organization": "collective",
  "initialize_git": ["1", "0"],
  "plone_version": "6.0",
  "__folder_name": "{{ cookiecutter.github_organization|lower }}.{{ cookiecutter.title|replace(' ', '')|replace('-', '_')|replace('.', '')|lower }}",
  "__year": "{% now 'local', '%Y' %}",
  "__profile_language": "en",
  "__version_package": "1.0.0a0",
  "__generator_date_long": "{% now 'utc', '%Y-%m-%d %H:%M:%S' %}",
  "__generator_signature": "This was generated by the [cookieplone-templates documentation_addon template](https://github.com/plone/cookieplone-templates/tree/main/documentation_addon) on {{ cookiecutter.__generator_date_long }}",
  "__documentation_addon_format": "1",
  "__prompts__": {
    "title": "Addon Title",
    "description": "A short description of your addon",
    "github_organization": "GitHub Username or Organization",
    "author": "Author",
    "email": "Author E-mail",
    "initialize_git": {
      "__prompt__": "Initialize Git Repository?",
      "1": "Yes",
      "0": "No"
    }
  },
  "_copy_without_render": [
  ],
  "_extensions": [
    "cookieplone.filters.latest_plone",
    "cookieplone.filters.use_prerelease_versions",
    "cookieplone.filters.pascal_case",
    "cookieplone.filters.package_namespaces",
    "cookieplone.filters.package_path"
  ],
  "__cookieplone_repository_path": "",
  "__cookieplone_template": ""
}
